{
  "name": "mexc_parser",
  "version": "1.0.0",
  "description": "Multi-exchange position tracker for MEXC and Gate.io",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "dev": "NODE_ENV=development node main.js",
    "build": "rollup -c && chmod +x dist/bundle.js && npm run build:copy-deps && echo 'âœ… Production build created in dist/'",
    "build:copy-deps": "cp -r node_modules dist/ 2>/dev/null || echo 'Skipping node_modules copy'",
    "build:start": "cd dist && node bundle.js",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mexc",
    "gate.io",
    "crypto",
    "position-tracker",
    "telegram-bot"
  ],
