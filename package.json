  "type": "commonjs",
  "dependencies": {
    "crypto-js": "4.2.0",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "node-telegram-bot-api": "0.67.0",
    "socket.io-client": "4.8.3",
    "ws": "8.19.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.8",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.3.1",
    "@rollup/plugin-terser": "0.4.4",
    "rollup": "4.55.2"
  }
}
